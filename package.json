{
  "name": "html5ever",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "html5ever",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.11.4",
    "@types/node": "^18.7.18",
    "@types/tap": "^15.0.7",
    "ava": "^4.3.0",
    "nodemon": "^2.0.20",
    "tap": "^16.3.0",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --dts=types/generated.d.ts && yarn merge-dts",
    "build:debug": "napi build --platform --dts=types/generated.d.ts && yarn merge-dts",
    "build:json-debug": "napi build --platform --dts=types/generated.d.ts --cargo-flags=\"--message-format=json --quiet\" && yarn merge-dts",
    "merge-dts": "cat types/generated.d.ts types/additional.d.ts > index.d.ts",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "yarn build:debug && tap",
    "test:update": "yarn build:debug && tap --snapshot",
    "version": "napi version",
    "watch:test": "nodemon --exec \"yarn test\"",
    "watch:test:update": "nodemon --exec \"yarn test:update\""
  },
  "packageManager": "yarn@3.2.3",
  "tap": {
    "coverage": false
  }
}